#: password_rules/password_rules.php:35
#, php-format
msgid "<strong>ERROR</strong>: Your new password must be at least %d characters long."
msgstr "<strong>ERREUR</strong>: Votre nouveau mot de passe doit contenir au moins %d caractères."

#: password_rules/password_rules.php:42
msgid "<strong>ERROR</strong>: Your new password must contain at least one uppercase letter and one lowercase letter."
msgstr "<strong>ERREUR</strong>: Votre nouveau mot de passe doit contenir au moins une minuscule et une majuscule."

#: password_rules/password_rules.php:45
msgid "<strong>ERROR</strong>: Your new password must contain at least one letter."
msgstr "<strong>ERREUR</strong>: Votre nouveau mot de passe doit contenir au moins une lettre."

#: password_rules/password_rules.php:52
msgid "<strong>ERROR</strong>: Your new password must contain at least one numeric character."
msgstr "<strong>ERREUR</strong>: Votre nouveau mot de passe doit contenir au moins une décimale."

#: password_rules/password_rules.php:59
msgid "<strong>ERROR</strong>: Your new password must contain at least one punctuation character among the following."
msgstr "<strong>ERREUR</strong>: Votre nouveau mot de passe doit contenir au moins un caractère de ponctuation parmis les suivants."

#: password_rules/password_rules_admin.php:14
msgid "Password Rules settings"
msgstr "Configuration de Password Rules"

#: password_rules/password_rules_admin.php:21
msgid "Minimum password length"
msgstr "Longueur minimale du mot de passe"

#: password_rules/password_rules_admin.php:30
msgid "Leave at '0' for no password length limit."
msgstr "Laisser à \"0\" pour ne pas définir de longueur minimale."

#: password_rules/password_rules_admin.php:34
msgid "Letter character required"
msgstr "Lettre requise"

#: password_rules/password_rules_admin.php:40
msgid "No letter required"
msgstr "Aucune lettre requise"

#: password_rules/password_rules_admin.php:41
msgid "At least one letter"
msgstr "Au moins une lettre requise"

#: password_rules/password_rules_admin.php:42
msgid "At least one lowercase letter and one uppercase letter"
msgstr "Au moins une minuscule et une majuscule"

#: password_rules/password_rules_admin.php:46
msgid "Require at least one digit"
msgstr "Au moins une décimale requise"

#: password_rules/password_rules_admin.php:52
msgid "Require punctuation symbole"
msgstr "Symbole de ponctuation requis"

#: password_rules/password_rules_admin.php:55
msgid "Require at least one of the following characters"
msgstr "Au moins un des caractères de ponctuation suivants est requis."

